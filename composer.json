{
    "name": "2amigos/yii2-highcharts-widget",
    "description": "Highcharts JS widget for Yii2.",
    "keywords": [
        "yii",
        "extension",
        "widget",
        "chart",
        "highcharts"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "bower-asset/highcharts-release": "4.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\highcharts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dosamigos\\highcharts\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}