{
    "name": "2amigos/yii2-highcharts-widget",
    "description": "Highcharts JS widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "chart",
        "highcharts"
    ],
    "homepage": "http://yiiwheels.com/extension/highcharts-widget",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/2amigos/yii2-highcharts-widget/issues",
        "source": "https://github.com/2amigos/yii2-highcharts-widget"
    },
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/highcharts-release": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "friendsofphp/php-cs-fixer": "^2.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\highcharts\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "bower-asset-library": "vendor/bower"
            },
            "vcs-driver-options": {
                "github-no-api": true
            },
            "git-skip-update": "2 days",
            "pattern-skip-version": "(-build|-patch)"
        }
    }
}
